    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-common": "^1.6.0",
    "typescript": "^3.3.3333"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
